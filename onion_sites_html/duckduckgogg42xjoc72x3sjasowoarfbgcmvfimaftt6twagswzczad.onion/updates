};</script><script>(function bootstrapTheme() {
  // This code needs to run on legacy browsers (so ES5 only)
  var supportsCssCustomProperties = window.CSS && window.CSS.supports && window.CSS.supports("color", "var(--test, red)");

  if (!supportsCssCustomProperties) {
    // hide content untill we can load `css-vars-ponyfill`
    document.documentElement.classList.add("invisible");
  } else {
    // set CSS variables to the right value (before any rendering) to avoid a flash of inaccurate color theme
    // eslint-disable-next-line no-inner-declarations
    function getInitialTheme() {
      var themeSettingName = "kae";
      var themeSettingCookieName = "ae";
      var themeSettingValueLight = "-1";
      var themeSettingValueDark = "d";
      var themeSettingValueTerminal = "t";
      var themeKeyDark = "dark";
      var themeKeyLight = "light"; // Helper function to get search params from URL

      function getParamFromURL(url, paramName) {
        var regex = new RegExp("[?&]" + paramName + "=([^&#]*)");
        var match = url.match(regex);
        return match && decodeURIComponent(match[1]);
      } // Prioritize URL params over Cookie value


      var param = getParamFromURL(document.URL, themeSettingName);

      if (param) {
        if (param === themeSettingValueLight) {
          return themeKeyLight;
        }

        if (param === themeSettingValueDark || param === themeSettingValueTerminal) {
          return themeKeyDark;
        }
      } // Fallback to cookie if theme setting not in URL params


      var cookies = document.cookie.split(/; */);
      var themeCookie;

      for (var i = 0; i < cookies.length; i++) {
        var cookie = cookies[i];
        var re = new RegExp("^" + themeSettingCookieName + "=");

        if (re.test(cookie)) {
          themeCookie = cookie;
          break;
        }
      }

      if (themeCookie) {
        var persisted = themeCookie.split("=")[1];

        if (persisted === themeSettingValueLight) {
          return themeKeyLight;
        }

        if (persisted === themeSettingValueTerminal) {
          return themeKeyDark;
        }
      } // Lastly, check prefers-color-scheme setting


      var mql = window.matchMedia("(prefers-color-scheme: dark)");
      return mql.matches ? themeKeyDark : themeKeyLight;
    }

