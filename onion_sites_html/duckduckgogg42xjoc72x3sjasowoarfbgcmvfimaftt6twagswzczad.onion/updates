  var url = "https://improving.duckduckgo.com/t/" + "static_err_global?" + Math.ceil(Math.random() * 1e7) + "&msg=" + encodeURIComponent(msg) + "&url=" + encodeURIComponent(source) + "&pathname=" + encodeURIComponent(window.location && window.location.pathname || "") + "&line=" + lineno + "&col=" + colno;

  try {
    if (window.navigator.sendBeacon) {
      window.navigator.sendBeacon(url);
    } else {
      var img = document.createElement("img");
      img.src = url;
    }
  } catch (e) {// noop
